         <ref name="la_model.graphicLike"/>
         <ref name="la_model.pPart.msdesc"/>
         <ref name="la_model.pPart.edit"/>
         <ref name="la_model.ptrLike"/>
         <ref name="la_model.lPart"/>
         <ref name="la_model.phrase.xml"/>
         <ref name="la_model.specDescLike"/>
         <ref name="la_model.pPart.data"/>
      </choice>
   </define>
   <define name="la_model.paraPart">
      <choice>
         <ref name="la_model.gLike"/>
         <ref name="la_model.lLike"/>
         <ref name="la_model.global"/>
         <ref name="la_model.inter"/>
         <ref name="la_model.phrase"/>
      </choice>
   </define>
   <define name="la_model.limitedPhrase">
      <choice>
         <ref name="la_model.hiLike"/>
